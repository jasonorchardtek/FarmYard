
entrypoint = "index.html"
modules = ["web:v2-20230623-0b7a606"]

[nix]
channel = "stable-23_11"

[deployment]
publicDir = "/"
deploymentTarget = "static"

[workflows]
runButton = "Serve static"

[[workflows.workflow]]
name = "Serve static"
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx static-web-server --config-file .config/static-web-server.toml"
waitForPort = 80

[[ports]]
localPort = 80
externalPort = 80
